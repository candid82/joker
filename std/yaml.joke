(ns
  ^{:go-imports []
    :doc "Implements encoding and decoding of YAML."}
  yaml)

(defn read-string
  "Parses the YAML-encoded data and return the result as a Joker value."
  {:added "1.0"
  :go "readString(s)"}
  [^String s])

(defn write-string
  "Returns the YAML encoding of v."
  {:added "1.0"
  :go "writeString(v)"}
  [^Object v])


